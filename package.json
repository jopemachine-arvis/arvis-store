{
  "name": "arvis-store",
  "version": "0.0.1",
  "description": "Retrieve info about Arvis extensions easily",
  "main": "dist/index.js",
  "bin": {
    "arvis-store": "dist/cli/main.js",
    "arvis-store-publish": "dist/lib/publish.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "rebuild": "rm -rf node_modules && npm i && npm run build",
    "lint": "tslint --project tsconfig.json --fix",
    "update-store": "node ./dist/lib/updateStore.js"
  },
  "keywords": [
    "arvis",
    "arvis-extension"
  ],
  "repository": {
    "url": "https://github.com/jopemachine/arvis-store"
  },
  "author": "jopemachine",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.5",
    "@types/semver": "^7.3.6",
    "get-npm-downloads": "^0.0.4",
    "husky": "^4.2.5",
    "latest-version": "^5.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@octokit/core": "^3.5.1",
    "@octokit/rest": "^18.6.6",
    "arvis-extension-validator": "^0.2.0",
    "chalk": "^4.1.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "meow": "^9.0.0",
    "octokit-commit-multiple-files": "^3.2.1",
    "ora": "^5.4.1",
    "semver": "^7.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  }
}
